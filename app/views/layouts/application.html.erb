<!DOCTYPE html>
<html>
  <head>
    <title>Gymhub</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
<% if user_signed_in? %>
  <body>
  	<%= render'layouts/navtop' %>
  	<div class="container">
    	<%= yield %>
    </div>
  </body>
<% else %>
  <body>
  <br>
  	<div class="container">
    	<%= yield %>
    </div>
  </body>
<% end %>
</html>
